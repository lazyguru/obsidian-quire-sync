    "lint": "eslint"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.14.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.19.8",
    "jest": "^29.7.0",
    "obsidian": "latest",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "tslib": "2.6.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "typed-rest-client": "^1.8.11"
  }
}
